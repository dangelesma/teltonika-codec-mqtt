{
  "name": "teltonika-codec-mqtt",
  "version": "2.3.0",
  "description": "Teltonika Codec 8/8E to MQTT with Codec 12 bidirectional commands support",
  "main": "./out/index.js",
  "scripts": {
    "build": "tsc && npm run copy-public",
    "copy-public": "node -e \"const fs=require('fs');const path=require('path');function copy(s,d){if(!fs.existsSync(d))fs.mkdirSync(d,{recursive:true});fs.readdirSync(s).forEach(f=>{const sp=path.join(s,f),dp=path.join(d,f);fs.statSync(sp).isDirectory()?copy(sp,dp):fs.copyFileSync(sp,dp)})};copy('src/public','out/public')\"",
    "start": "node out/index.js",
    "dev": "ts-node src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^20.8.10",
    "@types/ws": "^8.5.10",
    "complete-teltonika-parser": "^0.3.6",
    "dotenv": "^16.4.0",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.29.4",
    "mqtt": "^5.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.5"
  }
}
